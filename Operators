//Opeartor
//Binary operators.
//+ operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 20;
        System.out.println("Addition of 'a' and 'b' ="+ (a+b)); //you must use + between two sentences.
    }
}

//- Operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 20;
        System.out.println("Subtraction of 'a' and 'b' ="+ (a-b)); //you must use + between two sentences.
    }
}

//* Opeartor
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 20;
        System.out.println("Multiplication ="+ (a*b)); //you must use + between two sentences.
    }
}

// / Operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 20;
        System.out.println("Division ="+ (a/b)); //you must use + between two sentences.
    }
}

// % Operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 20;
        System.out.println("Modulo(Remainder) ="+ (a%b)); //you must use + between two sentences.
    }
}

//Unary Operators
//Pre increment 
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = ++a;
        System.out.println(a); 
        System.out.println(b);
    }
}

//Post Increment
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = a++;
        System.out.println(a); 
        System.out.println(b);
    }
}

//pre decrement (similar to pre increment)
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = --a;
        System.out.println(a); 
        System.out.println(b);
    }
}

//post decrement (Similar to post increment)
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = a--;
        System.out.println(a); 
        System.out.println(b);
    }
}

//Relational Operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 23;
        System.out.println(a==b);  //Here true is returned
    }
}

public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 22;
        System.out.println(a!=b);  //here result will be true as a is not equal to b
    }
}

public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 22;
        System.out.println(a<b);   //here output will be false
    }
}


public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 23;
        System.out.println(a<=b);  //here output will be true as both are equal
    }
}

//Logical Operators
//AND Operator
public class basic{
    public static void main(String args[]){
        int a = 23;
        int b = 23;
        System.out.println((a<=b) && (a==b));
    }
}

public class basic{
    public static void main(String args[]){
        System.out.println((2<6) && (1>2));
    }
}

public class basic{
    public static void main(String args[]){
        System.out.println((2<6) || (1>2));   //here output is true
    }
}

public class basic{
    public static void main(String args[]){
        System.out.println(!(1>2));  //here output will be true
    }
}

public class basic{
    public static void main(String args[]){
        int a = 10;
        a+=10;
        System.out.println(a);  //+= operator
    }
}

public class basic{
    public static void main(String args[]){
        int a = 10;
        a-=10;
        System.out.println(a);  //-= operator
    }
}




